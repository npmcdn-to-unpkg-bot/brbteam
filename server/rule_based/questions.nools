define Questions {
  text: '',
  tags: [],
  constructor: function(tags) {
    this.tags = tags;
  }
}

rule Rule1 {
  when {
    out: Questions "mongo" in out.tags || "db" in out.tags;

  }

  then {
    console.log("How does mongodb handle large binary files?");
  }
}

rule Rule2 {
  when {
    out: Questions "nodejs" in out.tags;

  }

  then {
    console.log("Explain what an event loop is?");
  }
}
